.certificate{
  background-color: #fff;
  border-radius: 16rpx;
  box-shadow: 0 0 14px 0 rgba(1, 18, 135, 0.08);
  padding: 30rpx;
  margin-bottom: 28rpx;
}
.certificate .cf-hd{
  padding-bottom: 20rpx;
}
.certificate .cf-bd{
  position: relative;
}
.certificate .cf-text{
  position: absolute;
  top: 260rpx;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  text-align: center;
  font-size: 20rpx;
}
.certificate .cf-text span{
  margin-bottom: 20rpx;
}

