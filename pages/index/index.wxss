.swiper-wrap {
  padding: 0 0 28rpx;
  border-radius: 28rpx;
  position: relative;
}
.swiper-wrap::after {
  content: '';
  display: block;
  position: absolute;
  bottom: 13rpx;
  left: 0;
  right: 0;
  height: 15rpx;
  background-color: #fff;
}
.home-swiper{
  height: 290rpx;
  overflow: hidden;
  border-radius: 28rpx;
}
wx-swiper .wx-swiper-dot{
  width: 10rpx;
  height: 10rpx;
  margin-left:-8rpx;
  margin-right:-8rpx;
}
wx-swiper .wx-swiper-dot-active{
  width: 28rpx;
  border-radius: 24rpx;
}
.sitemap-wrap{
  background: linear-gradient(132deg, #4AA0FC 0%, #694FFF 100%);
  border-radius: 16rpx;
  overflow: hidden;
}
.sitemap{
  background: url("data:image/png;base64,") no-repeat center bottom;
  background-size: 88%;
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
  padding: 26rpx 0;
  color: #fff;
}
.sitemap .sitemap-item{
  flex: 0 0 25%;
  text-align: center;
  line-height: 67rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
}
