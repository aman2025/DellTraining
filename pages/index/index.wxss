/* 三列 */
.sitemap.col3{
  justify-content: flex-start;
}
.sitemap.col3 .sitemap-item{
  flex: 0 0 33.33333%;
}
.notice-tip{
  background-color: #F6F6F6;
  border-radius: 16rpx;
  padding: 16rpx 20rpx;
  margin: 28rpx 0 0;
  display: flex;
}
.notice-tip .notice-tip-pic{
  width: 30rpx;
  margin-top: 6rpx;
}
.notice-tip .notice-tip-content{
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  padding-left: 10rpx;
  color: #5CB1F0;
}
.notice-tip .notice-tip-content em{
  margin-right: 50rpx;
}
.season-title{
  font-size: 32rpx;
  position: relative;
  text-align: center;
  font-weight: bold;
  margin: 32rpx 0 50rpx;
}
.season-title::before,
.season-title::after{
  content: '';
  position: absolute;
  top: 20rpx;
  width: 200rpx;
  height: 2rpx;
  background: linear-gradient(90deg, rgba(255, 255, 255, 0) 0%, #71BDF6 100%);
  border-radius: 2rpx;
}
.season-title::before{
  left: 88rpx;
}
.season-title::after{
  right: 88rpx;
  background: linear-gradient(90deg, #71BDF6 0%, rgba(255, 255, 255, 0) 100%);
}
/* 课程swiper */
.course-swiper-wrap{
  padding: 0 0 0 20rpx;
  margin-top: -20rpx;
  position: relative;
  background: linear-gradient(315deg, #694FFF 0%, #4AA0FC 100%);
}
.course-swiper{
  overflow: hidden;
  height: 622rpx;
}
.course-swiper .course-swiper-item{
  padding: 20rpx 8rpx;
  height: 100%;
}
.course-swiper .course-swiper-item .cst-content{
  padding: 30rpx;
  background: #FFFFFF;
  box-shadow: 0 0 16rpx 0 rgba(0, 0, 0, 0.08);
  border-radius: 16rpx;
  height: 100%;
}
/* 单独页面中，导航栏背景透明 */
.navbar {
  background-color: transparent !important;
}
.navbar-bg{
  top: 0;
}
.search-wrap{
  position: absolute;
  left: 31rpx;
  right: 201rpx;
  display: flex;
  top: 48rpx;
  z-index: 999999999;
}
.search{
  position: relative;
  height: 64rpx;
  line-height: 64rpx;
  background-color: #fff;
  border-radius: 32rpx;
  width: 100%;
  color: #CCDEEF;
  padding-left: 70rpx;
}
.search-bg{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 20rpx;
  left: 30rpx;
  z-index: 99;
}
.coure-more{
  width: 106rpx;
  background-color: #fff;
  box-shadow: 0 0 16rpx 0 rgba(0, 0, 0, 0.08);
  border-radius: 16rpx;
  height: calc(100% - 40rpx);
  position: absolute;
  right: -106rpx;
  top:20rpx;
  z-index: 99;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  color: #B3CBE1; 
  font-size: 22rpx;
  transition: right 0.31s linear 0.1s;
}
.btn {
  width: 122rpx !important;
}
/*首页跑马灯效果*/
@keyframes around {
  from {
   margin-left: 100%;
  }
  to {
   /* var接受传入的变量 */
   margin-left: var(--marqueeWidth--);
  }
  }
   
 .marquee_container{
  height: 50rpx;
  line-height: 44rpx;
  position: relative;
  width: 100%;
  margin-top:0rpx;
 }
 .marquee_text{
  font-size: 28rpx;
  display: inline-block;
  white-space: nowrap;
  animation-name: around;
  animation-duration: 10s; /*过渡时间*/
  animation-iteration-count: infinite;
  animation-timing-function:linear;
 }
 .swp-posi{
   padding-top: 10rpx !important;
 }
 .piclist .content .c-hd{
  padding-top: 0;
  line-height: 1.1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
 }
 .piclist .content .c-remark{
  font-size: 22rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  padding: 8rpx 0;
}
.single3{
  padding-right: 28rpx !important;
}